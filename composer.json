{
  "name": "andersundsehr/server-send-events",
  "description": "Utility Package to help with the creation of text/event-stream",
  "license": "GPL-2.0-or-later",
  "type": "library",
  "authors": [
    {
      "name": "Matthias Vogel",
      "email": "m.vogel@andersundsehr.com"
    }
  ],
  "require": {
    "php": "~8.2.0",
    "psr/http-message": "^1.1 || ^2.0"
  },
  "require-dev": {
    "pluswerk/grumphp-config": "^6.8"
  },
  "autoload": {
    "psr-4": {
      "AUS\\ServerSendEvents\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp": true,
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "pluswerk/grumphp-config": true
    }
  }
}
